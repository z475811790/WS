		<p k="1004" v="登录成功" />
		<p k="1005" v="昵称是:" />
		<p k="1006" v="请输入聊天室号:" />
		<p k="1007" v="聊天室号码必须为正整数,请重新输入" />
		<p k="1008" v="请输入昵称:" />
	</languageConstant>
	<message>
		<p k="1001" v="语言包1" />
		<p k="1002" v="语言包2" />
	</message>
</map>